<testConf xmlns="http://www.flocklab.ethz.ch" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.flocklab.ethz.ch xml/flocklab.xsd">
	<generalConf>
		<name>FlockLab Tutorial 3: Blink</name>
		<description>Example app for FlockLab tutorial 3. The app does a binary count on the three external LEDs on the FlockLab observer.</description>
		<scheduleAsap>
			<durationSecs>300</durationSecs>
		</scheduleAsap>
		<emailResults>yes</emailResults>
	</generalConf>
	
	<targetConf>
		<obsIds>001 002 004 006 007 008 010 011 013 014 015 016 017 018 019 020 022 023 024 025 026 027 028 031 032 033 200 201 202 204</obsIds>
		<voltage>3.3</voltage>
		<dbImageId>6818</dbImageId>
	</targetConf>

	<gpioTracingConf>
		<obsIds>001 002 004 006 007 008 010 011 013 014 015 016 017 018 019 020 022 023 024 025 026 027 028 031 032 033 200 201 202 204</obsIds>
		<pinConf>
			<pin>LED1</pin>
			<edge>both</edge>
			<mode>continuous</mode>
		</pinConf>
		<pinConf>
			<pin>LED2</pin>
			<edge>both</edge>
			<mode>continuous</mode>
		</pinConf>
		<pinConf>
			<pin>LED3</pin>
			<edge>both</edge>
			<mode>continuous</mode>
		</pinConf>
	</gpioTracingConf>

</testConf>
